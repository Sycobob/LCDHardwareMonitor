Texture2D source;
SamplerState samplerState;

struct PixelFragment
{
	float4 PosH  : SV_POSITION;
	float4 Color : COLOR;
	float2 UV    : TEXCOORD;
};

float4 main(PixelFragment pIn) : SV_TARGET
{
	float4 sample = source.Sample(samplerState, pIn.UV);
	//float avg = (sample.r + sample.b + sample.g) / 3;
	//return float4(sample.rgb, 0.5);
	return float4(sample.rgb, 1.0);
}
